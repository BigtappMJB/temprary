{
  "name": "table-schema-generator",
  "version": "1.0.0",
  "description": "A React application for generating table schemas and Spring Boot code",
  "main": "index.js",
  "scripts": {
    "start-react": "webpack serve --mode development",
    "start": "concurrently \"npm run start-react\" \"npm run python-service\"",
    "build": "webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-python": "node test_python_service.js",
    "mock-api": "node mock-api.js",
    "python-service": "node server/start_python_service.js",
    "install-python-deps": "node install_python_deps.js",
    "dev": "concurrently \"npm run start-react\" \"npm run mock-api\" \"npm run python-service\"",
    "setup": "npm install && npm run install-python-deps"
  },
  "keywords": [
    "react",
    "table",
    "schema",
    "generator",
    "spring",
    "boot"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.1",
    "style-loader": "^3.3.2",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
